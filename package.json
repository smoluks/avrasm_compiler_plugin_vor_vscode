{
  "name": "avrasmcompiler",
  "displayName": "AVRASM compiler plugin",
  "description": "Add AVRASM and AVRDUDE call commands",
  "publisher": "smoluks",
  "repository": "https://github.com/smoluks/avrasm_compiler_plugin_vor_vscode",
  "version": "0.0.13",
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "AVR",
    "AVR Assembler",
    "AVRASM",
    "AVRDUDE"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "onCommand:avrasm.compile",
    "onCommand:avrasm.options",
    "onCommand:avrasm.flash",
    "onWebviewPanel:avrasm"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "avrasm.compile",
        "title": "AVRASM: Compile"
      },
      {
        "command": "avrasm.options",
        "title": "AVRASM: Settings"
      },
      {
        "command": "avrasm.flash",
        "title": "AVRASM: Flash"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.17.15",
    "@types/vscode": "^1.42.0",
    "glob": "^7.1.6",
    "mocha": "^6.2.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {}
}
