{
  "name": "avrasmcompiler",
  "displayName": "AVRASM compiler",
  "description": "AVRASM compile",
  "publisher": "smoluks",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:avrasm.compile",
    "onCommand:avrasm.options",
    "onWebviewPanel:avrasm"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "avrasm.compile",
        "title": "AVRASM: Compile"
      },
      {
        "command": "avrasm.options",
        "title": "AVRASM: Settings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.14.22",
    "@types/vscode": "^1.39.0",
    "glob": "^7.1.4",
    "mocha": "^6.2.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {}
}
